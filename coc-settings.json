{
    "omnisharp": {
            "command": "/opt/omnisharp-roslyn-bundled/run",
            "args": ["-lsp"],
            "filetypes": ["cs", "csproj", "sln"],
            "trace.server": "verbose"
    },
    "clangd.semanticHighlighting": "true",
    "clangd.path": "/usr/bin/clangd",

    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-openjdk/",
        }
    ]

}
